.ReactJsonSyntaxHighlighter
  pre
    font-size 9pt
    white-space pre-wrap
    white-space -moz-pre-wrap
    white-space -pre-wrap
    white-space -o-pre-wrap
    word-wrap break-word

   .string
     color green
   .number
     color darkorange
   .boolean
     color blue
   .null
     color magenta
   .key
     color red
